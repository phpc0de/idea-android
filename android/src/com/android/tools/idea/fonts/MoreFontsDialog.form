<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.android.tools.idea.fonts.MoreFontsDialog">
  <grid id="27dc6" binding="myContentPanel" custom-create="true" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="d003" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="empty">
          <size top="6" left="6" bottom="6" right="6"/>
        </border>
        <children>
          <component id="28cbf" class="com.intellij.ui.SearchTextField" binding="mySearchField" custom-create="true">
            <constraints border-constraint="West"/>
            <properties/>
          </component>
          <grid id="c7532" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="East"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="6d2ff" class="com.intellij.ui.components.JBLabel">
                <constraints border-constraint="West"/>
                <properties>
                  <text value="Source"/>
                </properties>
              </component>
              <component id="469ff" class="javax.swing.JComboBox" binding="myProvider">
                <constraints border-constraint="Center"/>
                <properties/>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <component id="fe193" class="com.intellij.ui.components.JBLabel" binding="myFontLabel">
        <constraints border-constraint="West"/>
        <properties>
          <text value="Fonts"/>
          <verticalAlignment value="1"/>
        </properties>
      </component>
      <grid id="8d392" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="27d2" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="empty">
              <font/>
              <size top="8" left="8" bottom="0" right="8"/>
            </border>
            <children>
              <nested-form id="ca9d1" form-file="com/android/tools/adtui/validation/ValidatorPanel.form" binding="myValidatorPanel" custom-create="true">
                <constraints border-constraint="South"/>
              </nested-form>
              <component id="66e11" class="com.intellij.ui.HyperlinkLabel" binding="myLicenseLabel">
                <constraints border-constraint="Center"/>
                <properties/>
              </component>
            </children>
          </grid>
          <grid id="42ef7" binding="myFontListPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="empty">
              <size top="0" left="8" bottom="0" right="4"/>
            </border>
            <children>
              <scrollpane id="7e593" class="com.intellij.ui.components.JBScrollPane" binding="myFontListScrollPane">
                <constraints border-constraint="Center"/>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="718fd" class="com.intellij.ui.components.JBList" binding="myFontList">
                    <constraints/>
                    <properties/>
                  </component>
                </children>
              </scrollpane>
            </children>
          </grid>
          <grid id="afae0" binding="myPreviewPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="East"/>
            <properties/>
            <border type="empty">
              <size top="0" left="4" bottom="0" right="8"/>
            </border>
            <children>
              <grid id="aec13" layout-manager="BorderLayout" hgap="0" vgap="12">
                <constraints border-constraint="North"/>
                <properties/>
                <border type="empty">
                  <size top="0" left="0" bottom="8" right="0"/>
                </border>
                <children>
                  <component id="d899a" class="com.intellij.ui.components.JBLabel">
                    <constraints border-constraint="South"/>
                    <properties>
                      <text value="Preview"/>
                    </properties>
                  </component>
                  <grid id="e3da3" binding="myCreateParams" layout-manager="BorderLayout" hgap="0" vgap="0">
                    <constraints border-constraint="Center"/>
                    <properties/>
                    <border type="none"/>
                    <children>
                      <component id="f8daf" class="com.intellij.ui.components.JBLabel" binding="myFontName">
                        <constraints border-constraint="West"/>
                        <properties>
                          <text value="Font Name:"/>
                          <verticalAlignment value="0"/>
                        </properties>
                      </component>
                      <component id="9b9a9" class="javax.swing.JTextField" binding="myFontNameEditor">
                        <constraints border-constraint="Center"/>
                        <properties>
                          <text value="droid_sans"/>
                        </properties>
                      </component>
                      <grid id="d9a27" binding="myDownloadable" layout-manager="GridLayoutManager" row-count="2" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
                        <margin top="0" left="0" bottom="0" right="0"/>
                        <constraints border-constraint="South"/>
                        <properties/>
                        <border type="none"/>
                        <children>
                          <component id="5ba79" class="javax.swing.JRadioButton" binding="myMakeDownloadable">
                            <constraints>
                              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                            </constraints>
                            <properties>
                              <selected value="true"/>
                              <text value="Create downloadable font"/>
                            </properties>
                          </component>
                          <component id="c4776" class="javax.swing.JRadioButton">
                            <constraints>
                              <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                            </constraints>
                            <properties>
                              <text value="Add font to project"/>
                            </properties>
                          </component>
                        </children>
                      </grid>
                    </children>
                  </grid>
                </children>
              </grid>
              <scrollpane id="4041b" class="com.intellij.ui.components.JBScrollPane">
                <constraints border-constraint="Center"/>
                <properties/>
                <border type="none"/>
                <children>
                  <component id="d9e0a" class="com.intellij.ui.components.JBList" binding="myFontDetailList">
                    <constraints/>
                    <properties/>
                  </component>
                </children>
              </scrollpane>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
  <buttonGroups>
    <group name="myDownloadableGroup">
      <member id="5ba79"/>
      <member id="c4776"/>
    </group>
  </buttonGroups>
</form>
