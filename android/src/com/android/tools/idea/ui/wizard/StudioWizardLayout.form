<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.android.tools.idea.ui.wizard.StudioWizardLayout">
  <grid id="27dc6" binding="myRootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="8aea9" binding="myHeaderPanel" layout-manager="GridLayoutManager" row-count="1" column-count="3" same-size-horizontally="false" same-size-vertically="true" hgap="16" vgap="-1">
        <margin top="0" left="8" bottom="8" right="8"/>
        <constraints border-constraint="North"/>
        <properties/>
        <border type="empty">
          <size top="20" left="10" bottom="20" right="10"/>
        </border>
        <children>
          <component id="780db" class="com.android.tools.adtui.ImageComponent" binding="myIcon">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
          </component>
          <component id="21ec7" class="com.intellij.ui.components.JBLabel" binding="myTitleLabel">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="6" hsize-policy="6" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <font size="24"/>
              <foreground color="-1"/>
              <text value="(Wizard Title)"/>
            </properties>
          </component>
          <component id="deb36" class="javax.swing.JLabel" binding="myStepIcon">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false">
                <minimum-size width="60" height="60"/>
                <preferred-size width="60" height="60"/>
                <maximum-size width="60" height="60"/>
              </grid>
            </constraints>
            <properties>
              <alignmentX value="0.5"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <name value="right_icon"/>
              <text value=""/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="d5619" binding="myCenterPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="empty">
          <size top="24" left="24" bottom="24" right="24"/>
        </border>
        <children/>
      </grid>
    </children>
  </grid>
</form>
