    a:orientation="vertical"
    a:layout_width="fill_parent"
    a:layout_height="fill_parent">

    <LinearLayout
        xmlns:a2="http://schemas.android.com/apk/res/android"
        style="@style/TitleBar"
        a2:id="@+id/header">
